{
  "name": "choclate",
  "version": "0.0.1",
  "manifest_version": 2,
  "description" : "simple game",
  "icons": { "hero": "images/hero.png", "villain": "images/villain.png", "": "images/zombie.png" },

  "omnibox": { "keyword" : "AAP" },

  "browser_action": {
    "default_icon": {
      "hero": "imagess/hero.png",
      "villain": "images/villain.png"
    },
    "default_title": "That's the tool tip",
    "default_popup": "browseraction/popup.html"
  },

  "background": {
    "scripts": ["js/Const.js"],
    "scripts": ["js/Game.js"],
    "scripts": ["js/Monster.js"],
    "scripts": ["js/Hero.js"],

    "persistent": false
  },

  "chrome_url_overrides" : {
    "newtab": "newtab/newtab.html"
  },

  
  "devtools_page": "devtools/devtools.html"
}